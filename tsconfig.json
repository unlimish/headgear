{
	"compilerOptions": {
	  "target": "es6",
	  "module": "commonjs",
	  "allowJs": true,
	  "outDir": "./dist",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
	  "lib": [
		"DOM",
		"dom.iterable"
	  ]
	},
	"include": [
	  "./*.ts",
	  "./src/*.ts", "index.js",
	],
	"exclude": [
	  "node_modules",
	  "**/*.test.ts"
	]
  }
